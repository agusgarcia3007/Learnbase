    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "@langfuse/client": "^4.4.9",
    "@langfuse/otel": "^4.4.9",
    "@langfuse/tracing": "^4.4.9",
    "@napi-rs/canvas": "^0.1.84",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@types/pdfkit": "^0.17.4",
    "ai": "^6.0.1",
    "bullmq": "^5.66.2",
    "color": "^5.0.3",
    "colorjs.io": "^0.5.2",
    "dotenv": "^17.2.3",
