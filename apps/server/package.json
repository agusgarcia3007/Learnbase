{
  "name": "learnbase-server",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run dist/index.js",
    "typecheck": "tsc --noEmit",
    "build": "bun build src/index.ts --outdir dist --target bun --external '@bull-board/*' --external '@napi-rs/*'",
    "test": "bun test",
