    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "resend": "^6.5.2",
    "stripe": "^20.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/color": "^4.2.0",
    "@types/pdf-parse": "^1.1.5",
